include ../layouts/_Settings.pug

doctype html
html(lang="en")
  head
    -
      path = '/contact.html'
      pageTitle = 'MAP 地圖導覽';
      infoTitle = '地圖導覽的資訊長標題';
      description = '地圖導覽的描述'
      image = '/img/og/map-og.jpg'

    include ../layouts/_Head.pug
    
  body
    include ../layouts/_Header.pug

    main(class="l-main")
      div(class="l-hero-inner" data-aos="fade-down")
        h2(class="text-3xl fw-900") MAP
        
      section(class="l-contact")
        div(class="container")
          div(class="l-contact__map-wrap")
            iframe(
              src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3682.9061297135568!2d120.27933465080466!3d22.619980285085497!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x346e047620e1c45b%3A0xe6d6d1bd1c764df2!2z6aeB5LqM6Jed6KGT54m55Y2A!5e0!3m2!1szh-TW!2stw!4v1621531731204!5m2!1szh-TW!2stw"
              allowfullscreen=""
              loading="lazy"
              class="l-contact__map"
            )

    include ../layouts/_Footer.pug